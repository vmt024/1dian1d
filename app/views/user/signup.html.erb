<h2>我要注册</h2>

<%= form_for @user, :url => {:controller => :user, :action=>:create},:html => { :multipart => true, :id=>"signup_form" } do |f| %>
  <%= render :partial => "form", :object => f %>
  <%= f.submit "注册" %>
<% end %>
<script type="text/javascript">
  $("#signup_form").validationEngine();
</script>
